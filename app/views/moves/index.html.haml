- widget_box "Moves" do
  
  %table#moves
    %thead
      %tr
        %th Move
        %th Difficulty
        %th Category
        %th Start hands
        %th Finish hands
    %tbody
      = render :partial => 'move', :collection => @moves

:javascript
  $(document).ready(function() {
    $('#moves').tableFilter();
  });
